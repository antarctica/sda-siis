{
  "name": "sea-ice-information-service",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen && cd .. && husky install app_v2/.husky",
    "dev": "vite",
    "build": "tsc && vite build && npm run build-docs",
    "lint": "eslint . --fix",
    "lint:timing": "eslint . --debug",
    "studio": "panda studio",
    "preview": "vite preview",
    "new-component": "new-component",
    "check-types": "tsc --noemit -p tsconfig.json",
    "generate-api-types": "openapi-typescript ../api/api/siis_api.yml -o ./src/types/api.d.ts && npm run lint",
    "generate-docs": "openapi-to-md ../api/api/siis_api.yml ../docs/api.md",
    "build-docs": "npm run generate-docs && npm run generate-api-types"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@arcgis/core": "^4.30.7",
    "@arcgis/map-components-react": "^4.30.7",
    "@fontsource-variable/inter": "^5.1.0",
    "@internationalized/date": "^3.5.6",
    "@internationalized/number": "^3.5.4",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-router": "^1.52.5",
    "@tanstack/router-zod-adapter": "^1.52.5",
    "@xstate/react": "^4.1.3",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.24.0",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.4",
    "new-component": "^5.0.2",
    "openapi-fetch": "^0.12.2",
    "react": "^18.2.0",
    "react-aria-components": "^1.3.3",
    "react-dom": "^18.2.0",
    "react-pro-sidebar": "^1.1.0",
    "react-redux": "^9.1.2",
    "react-stately": "^3.33.0",
    "swr": "^2.2.5",
    "swr-openapi": "^5.0.0",
    "usehooks-ts": "^3.1.0",
    "vaul": "^1.0.0",
    "vite-tsconfig-paths": "^4.3.2",
    "wkx": "^0.5.0",
    "xstate": "^5.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.10.0",
    "@pandacss/dev": "^0.36.1",
    "@pandacss/eslint-plugin": "^0.2.0",
    "@pandacss/studio": "^0.46.1",
    "@tanstack/router-devtools": "^1.52.5",
    "@tanstack/router-plugin": "^1.52.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/geojson": "^7946.0.14",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "husky": "^8.0.0",

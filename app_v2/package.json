{
  "name": "sea-ice-information-service",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "npm run build-docs && panda codegen && cd .. && husky install app_v2/.husky",
    "dev": "vite",
    "update-deps-check": "taze",
    "update-deps": "taze --write",
    "build": "tsc && vite build",
    "lint": "eslint . --fix",
    "lint:timing": "eslint . --debug",
    "studio": "panda studio",
    "preview": "vite preview",
    "new-component": "new-component",
    "check-types": "tsc --noemit -p tsconfig.json",
    "generate-api-types": "openapi-typescript ../api/api/siis_api.yml -o ./src/types/api.d.ts && eslint ./src/types/api.d.ts --fix --config eslint.config.mjs",
    "generate-docs": "openapi-to-md ../api/api/siis_api.yml ../docs/api.md",
    "build-docs": "npm run generate-docs && npm run generate-api-types"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@arcgis/core": "^4.31.3",
    "@arcgis/map-components-react": "^4.31.5",
    "@internationalized/date": "^3.5.6",
    "@internationalized/number": "^3.5.4",
    "@radix-ui/react-accordion": "^1.2.1",
    "@reduxjs/toolkit": "^2.3.0",
    "@tanstack/react-router": "^1.82.0",
    "@xstate/react": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.24.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.17",
    "new-component": "^5.0.3",
    "openapi-fetch": "^0.13.0",
    "react": "^18.3.1",
    "react-aria-components": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-pro-sidebar": "^1.1.0",
    "react-redux": "^9.1.2",
    "react-stately": "^3.33.0",
    "swr": "^2.2.5",
    "swr-openapi": "^5.1.0",
    "usehooks-ts": "^3.1.0",
    "vaul": "^1.1.1",
    "vite-tsconfig-paths": "^5.1.2",
    "xstate": "^5.19.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.15.0",
    "@pandacss/dev": "^0.48.0",
    "@pandacss/eslint-plugin": "^0.2.0",
    "@pandacss/studio": "^0.48.0",
    "@tanstack/router-devtools": "^1.82.0",
    "@tanstack/router-plugin": "^1.81.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/geojson": "^7946.0.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.01",
    "lint-staged": "^15.2.10",
    "openapi-to-md": "^1.0.24",
    "openapi-typescript": "^7.4.3",
    "prettier": "^3.3.3",
    "taze": "^0.18.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.27.2"
  }
}
